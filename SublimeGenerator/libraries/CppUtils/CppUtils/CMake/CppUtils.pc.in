prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
includedir=@CMAKE_INSTALL_FULL_INCLUDEDIR@
libdir=@CMAKE_INSTALL_FULL_LIBDIR@

Name: CPPUTILS
Description: Base engine and libraries for project.
Version: @CPPUTILS_VERSION@
URL: n/a
Requires.private: @CPPUTILS_PKG_CONFIG_REQUIRES_PRIVATE@
Libs: -L${libdir} -l@CPPUTILS_LIB_NAME@@CPPUTILS_LIB_NAME_SUFFIX@
Libs.private: @CPPUTILS_PKG_CONFIG_LIBS_PRIVATE@
Cflags: -I${includedir}
